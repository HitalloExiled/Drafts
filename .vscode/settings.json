        "**/Library/**/*.js": true,
        "**/Library/**/*.js.map": true,
        "**/bin": true,
        "**/obj": true,
        "**/**.njsproj": true,
        //"**/node_modules": true,
        "**/.vs": true,
        "**/*.sln": true
    },
    "workbench.iconTheme": "vs-seti"
