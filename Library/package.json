{
  "name": "surface-drafts",
  "version": "0.0.0",
  "description": "Library",
  "main": "./build/es5/index.js",
  "author": "HitalloExiled",
  "dependencies": {
    "@types/node": "^7.0.4",
    "cpx": "^1.5.0",
    "rimraf": "^2.5.4",
    "ts-loader": "^2.0.0",
    "ts-node": "^2.0.0",
    "tslib": "^1.5.0",
    "typescript": "^2.2.0",
    "webpack": "^2.2.0"
  },
  "scripts": {
    "prepublish": "npm run build",
    "build": "npm run clean && tsc -p ./source/tsconfig-es5.json | tsc -p ./source/tsconfig-es6.json && npm run copy-files",
    "clean": "rimraf ./build",
    "watch-es5": "tsc -p ./source/tsconfig-es5.json -watch",
    "watch-es6": "tsc -p ./source/tsconfig-es6.json -watch",
    "watch-all": "npm run watch-es5 | npm run watch-es6",
    "copy-files": "cpx ./source/**/*.{d.ts,html,scss} ./build/es5 && cpx ./source/**/*.{d.ts,html,css} ./build/es6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HitalloExiled/Drafts.git"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/HitalloExiled/Drafts/issues"
  },
  "homepage": "https://github.com/HitalloExiled/Drafts#readme"
}
